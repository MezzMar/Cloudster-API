{
   "name": "cloudster",
   "version": "1.0.0",
   "description": "This is the back end of out application, Cognitive Realm, A simple, though overcomplicated (For academic purposes), way to share files in your local network.",
   "private": true,
   "main": "./bin/index.ts",
   "scripts": {
      "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
      "build": "tsc",
      "prestart": "npm run build",
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "node ./bin/index.ts",
      "dev": "SET DEBUG=helloworld:* & nodemon ./bin/index.ts"
   },
   "keywords": [
      "Oropeza",
      "Luis",
      "Fernandez",
      "cloud",
      "files",
      "sharing",
      "LAN"
   ],
   "authors": [
      "Gary Nadir (MezzMar_)",
      "Diego Lopez (Diomes)"
   ],
   "license": "ISC",
   "dependencies": {
      "cookie-parser": "~1.4.4",
      "debug": "~2.6.9",
      "express": "~4.16.1",
      "http-errors": "~1.6.3",
      "jade": "~1.11.0",
      "morgan": "~1.9.1",
      "jsonwebtoken": "^8.5.1",
      "sqlite3": "^4.1.1",
      "multer": "^1.4.1",
      "body-parser": "^1.19.0"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/express": "^4.17.2",
      "@types/http-errors": "^1.6.3",
      "@types/jsonwebtoken": "^8.3.5",
      "@types/morgan": "^1.7.37",
      "@types/node": "^13.7.0",
      "nodemon": "^2.0.1",
      "ts-node": "^8.5.3",
      "tslint": "^6.0.0",
      "typescript": "^3.7.5"
   },
   "pkg": {
      "scripts": [
         "lib/index.js",
         "lib/sqlite3.js",
         "lib/trace.js",
         "lib/binding/*"
      ],
      "assets": [
         "lib/binding/*",
         "build/*",
         "build/**/*"
      ]
   }
}
